<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<GridPane xmlns:fx="http://javafx.com/fxml/1"
          fx:id="menu1Grid" alignment="center" prefHeight="600.0"
          prefWidth="400.0"
          xmlns="http://javafx.com/javafx/10.0.2" fx:controller="sample.Menu1Controller"
          stylesheets="@sample/resources/style.css">

    <VBox>
        <HBox>
            <Label alignment="TOP_CENTER" fx:id="titleImage" styleClass="titleImage"/>
        </HBox>
        <HBox>
            <Button text="Play" fx:id="playButton" onAction="#goToMenu2"/>
        </HBox>


    </VBox>
</GridPane>
